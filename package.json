    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@types/ramda": "^0.29.6",
    "@types/serialize-javascript": "^5.0.3",
    "@types/socket.io-client": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "axios": "^1.5.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.10",
    "eslint": "^8.51.0",
