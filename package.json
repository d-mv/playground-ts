    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "form-data": "^4.0.0",
    "fp-ts": "^2.16.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ramda": "^0.29.1",
    "rxjs": "^7.8.1",
    "serialize-javascript": "^6.0.1",
