  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll --unhandled-rejections=strict",
    "dev:prod": "export NODE_ENV=production && ts-node-dev ./src/index.ts",
    "dev:ts": "ts-node-dev ./src/index.ts",
    "dev": "node --watch-preserve-output --watch-path=./src --env-file=development.env --trace-warnings ./src/index.mjs",
    "start": "ts-node ./src/index.ts",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src"
  },
  "dependencies": {
